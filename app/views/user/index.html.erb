
<h1>ユーザー一覧</h1>
<table class="table1" align="left">
    <thead>
        <tr>
            <th scope="col" style="width:140px">名前</th>
                <th>メールアドレス</th>
                <th>管理者</th>
                <th>登録日</th>
                <th scope="col"></th>
            </tr>
        </thead>
        <tbody>
        <% @users.each do |user|%>
        <tr>
            <td><%= user.name %></td>
            <td><%= user.email %></td>
            <td><%= user.admin %></td>
            <td><%#%= user.created_at %></td>
            <td>
            <%= link_to '削除', "/user/#{user.id}", method: :delete %></td>
        </tr>
        <% end %>
</table>